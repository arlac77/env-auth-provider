{
  "name": "env-auth-provider",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/env-auth-provider.mjs",
  "module": "src/env-auth-provider.mjs",
  "description": "Provide credentials from environment variables",
  "keywords": [
    "auth-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*.mjs' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/env-auth-provider.mjs --section=API",
    "lint": "documentation lint src/env-auth-provider.mjs",
    "posttest": "markdown-doctest && npm run prepare",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^2.2.0",
    "c8": "^5.0.2",
    "documentation": "^12.1.0",
    "esm": "^3.2.25",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^15.13.19"
  },
  "engines": {
    "node": ">=10.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/env-auth-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/env-auth-provider/issues"
  },
  "homepage": "https://github.com/arlac77/env-auth-provider#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  }
}

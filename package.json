{
  "name": "env-auth-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/env-auth-provider.mjs"
  },
  "description": "Provide credentials from environment variables",
  "keywords": [
    "auth-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "devDependencies": {
    "ava": "^5.1.1",
    "c8": "^7.12.0",
    "documentation": "^14.0.1",
    "semantic-release": "^20.0.3"
  },
  "engines": {
    "node": ">=16.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/env-auth-provider"
  },
  "bugs": {
    "url": "https://github.com/arlac77/env-auth-provider/issues"
  },
  "homepage": "https://github.com/arlac77/env-auth-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}

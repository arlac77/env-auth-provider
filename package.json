{
  "name": "env-auth-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/env-auth-provider.js",
  "module": "src/env-auth-provider.mjs",
  "description": "Provide credentials from environment variables",
  "keywords": [
    "auth-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 --temp-directory build/coverage ava && c8 report -r lcov --temp-directory build/coverage",
    "docs": "documentation readme src/env-auth-provider.mjs --section=API",
    "lint": "documentation lint src/env-auth-provider.mjs",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "test": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.1",
    "c8": "^3.2.1",
    "documentation": "^9.1.1",
    "esm": "^3.0.84",
    "markdown-doctest": "^0.9.1",
    "rollup": "^1.0.1",
    "rollup-plugin-cleanup": "^3.1.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-executable": "^1.3.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "semantic-release": "^15.13.2"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/env-auth-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/env-auth-provider/issues"
  },
  "homepage": "https://github.com/arlac77/env-auth-provider#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.js",
      "tests/*-test.mjs"
    ],
    "extensions": [
      "js",
      "mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}

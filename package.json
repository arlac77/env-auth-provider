{
  "name": "env-auth-provider",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/env-auth-provider.mjs",
  "exports": {
    ".": "./src/env-auth-provider.mjs",
    "./": "./"
  },
  "description": "Provide credentials from environment variables",
  "keywords": [
    "auth-provider"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "ava --timeout 2m",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/env-auth-provider.mjs --section=API",
    "lint": "documentation lint ./src/env-auth-provider.mjs"
  },
  "devDependencies": {
    "ava": "^3.8.1",
    "c8": "^7.1.1",
    "documentation": "^12.3.0",
    "semantic-release": "^17.0.7"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/env-auth-provider.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/env-auth-provider/issues"
  },
  "homepage": "https://github.com/arlac77/env-auth-provider#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
